{
  "name": "envio",
  "version": "0.1.0",
  "scripts": {
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "mocha": "ts-mocha test/**/*.ts",
    "codegen": "envio codegen",
    "dev": "envio dev",
    "start": "envio start",
    "test": "pnpm mocha",
    "envio:dev": "envio codegen && docker network create envio-shared || true && docker compose -f generated/docker-compose.yaml -f generated/envio.override.yml up -d"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "envio": "2.30.1",
    "ethers": "^6.15.0",
    "ioredis": "^5.8.2"
  },
  "optionalDependencies": {
    "generated": "./generated"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
